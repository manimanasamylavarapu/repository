		<project.build.javaVersion>1.7</project.build.javaVersion>
		<jdkVersion>1.7</jdkVersion>
		<maven.compile.targetLevel>1.5</maven.compile.targetLevel>
		<maven.compile.sourceLevel>${jdkVersion}</maven.compile.sourceLevel>
	</properties>

